{
    "name": "bensondevs/laravel-boiler-plate",
    "description": "Laravel Boiler Plate to start development with ease",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bensondevs\\LaravelBoilerPlate\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Simeon Bensona",
            "email": "bensondevs@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload-dev": {
        "files": [
            "./src/Helpers/Helpers.php",
            "./src/Helpers/UrlHelpers.php",
            "./src/Helpers/HtmlHelpers.php",
            "./src/Helpers/FileHelpers.php",
            "./src/Helpers/StringHelpers.php",
            "./src/Helpers/DateTimeHelpers.php",
            "./src/Helpers/EncryptionHelpers.php"
        ],
        "psr-4": {
            "Bensondevs\\LaravelBoilerPlate\\Tests\\": "src/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Bensondevs\\LaravelBoilerPlate\\LaravelBoilerPlateServiceProvider"
            ]
        }
    }
}
